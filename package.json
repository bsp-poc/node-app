{
  "name": "@cph-poc/npm-test-run",
  "version": "0.0.4",
  "private": false,
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "lint-js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:report": "npm run lint:js -- --output-file lint-js-report.json --format json .",
    "lint:pkg-json": "lint-pkg-json --ignorePath .gitignore",
    "lint:staged": "lint-staged",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "applicationinsights": "^2.6.0",
    "body-parser": "~1.20.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "2.5.7",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "~4.18.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.22",
    "obtain-github-app-installation-access-token": "^1.1.0",
    "serve-favicon": "~2.4.2"
  },
  "description": "## Document * Clone this repo: ``` git clone https://github.com/bhanushalimahesh3/node-website.git ``` * Install dependencies using [npm](https://www.npmjs.com/) javascript package manager: ``` npm install ``` * Start node server with [nodemon](https://nodemon.io/): ``` nodemon start ``` * Tune to url: ``` http://localhost:3000 ```",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cph-poc/npm-test-run.git"
  },
  "keywords": [
    "poc"
  ],
  "author": "ajlajom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cph-poc/npm-test-run/issues"
  },
  "homepage": "https://github.com/cph-poc/npm-test-run#readme",
  "devDependencies": {
    "eslint": "8.38.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "43.0.5"
  }
}
